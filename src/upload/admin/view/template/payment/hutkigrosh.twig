{{ header }}{{ column_left }}
<div id="content">
	<div class="page-header">
		<div class="container-fluid">
			{% if old_style == false %}
				<div class="float-end">
				<a href="{{ cancel }}" data-bs-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-light"><i class="fa-solid fa-reply"></i></a>
			</div>
			{% endif %}
			<div class="float-left">
				{% if old_style == true %}
				<a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a>
				{% endif %}
				{% if old_style == true %}<div style="display: flex;">{% endif %}<h1><img src="data:image/png;base64," alt=""/>{{ heading_title }}</h1>{% if old_style == true %}</div>{% endif %}
				<ol class="breadcrumb">
					{% for breadcrumb in breadcrumbs %}
						<li class="breadcrumb-item"><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
					{% endfor %}
				</ol>
			</div>
		</div>
	</div>

	<div class="container-fluid">
		{% if messages|length %}
			{{ messages }}
		{% endif %}
		{{ configForm.generate() }}
	</div>
</div>
{{ footer }}
